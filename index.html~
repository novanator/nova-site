    <script>
    //shifting images
    HEADER_IMGS = {
	    'biotech-img': ['Images/green.png', 'Images/biotech.jpg'],
	    'cs-img': ['Images/yellow.png', 'Images/cs.png'],
	    'med-img': ['Images/red.png', 'Images/OPG.jpg'],
	    'ee-img': ['Images/blue.png', 'Images/el.png'],
	    'linkedin-img': ['Images/linkedin-dark.png', 'Images/linkedin.png'],
	    'git-img': ['Images/git-dark.png', 'Images/git.png'],
    };
    
    TAB_PAIRINGS = {
        "about-nav": "#about",
        "research-nav": "#research",
        "leadership-nav": "#leadership",
        "projects-nav": "#projects",
    };

    //Displaying pages in header
    function displayPage(pageID){ 
        $("#top-nav").find("li").removeClass("active"); 
        $("#main-content").html("").append($("#" + pageID).html()); 
        window.location.hash = pageID;
        window.document.title = "Nicole Nova | " + pageID;
    };
    
    //Navigation in the browser
    window.onhashchange = function (){
        console.log('changing hash!');
        var pageID = window.location.hash.slice(1);
        displayPage(pageID);
    };


    //jQuery
    $(document).ready(function(){
        //shifting images in header
	    $(".header-img").hover(
		    function() { $(this).attr('src', HEADER_IMGS[$(this).attr("id")][0]); }, //in
		    function() { $(this).attr('src', HEADER_IMGS[$(this).attr("id")][1]); } //out
	    );

        //Switching contents on the page, displaying pages on navigation bar
        $("#top-nav").on("click", "li a", function() {
            $(this).parent().addClass("active");
            var id = $(this).attr("id");
            $("#top-nav").find("li").removeClass("active");
            $("#main-content").html("").append($(TAB_PAIRINGS[id]).html());
            window.location.hash = TAB_PAIRINGS[id];
            window.document.title = "Nicole Nova | " + TAB_PAIRINGS[id].slice(1);
        });
        
        $("#header-banner, #home-nav").on("click", function(){ 
            displayPage("home");
        });

        $("#contact-nav").on("click", function(){
            displayPage("contact");
        });
        
        $("#biotech-img").on("click", function(){ 
            displayPage("biotech");
        });

        $("#cs-img").on("click", function(){ 
            displayPage("cs");
        });

        $("#med-img").on("click", function(){ 
            displayPage("med");
        });

        $("#ee-img").on("click", function(){ 
            displayPage("ee");
        });

        $("#main-content").on("click", "#view1, #view2, #view3", function(){
            displayPage("research");
        });

        $("#main-content").on("click", "#view4", function(){
            displayPage("rsi-panel");
        });

        $("#main-content").on("click", "#here-education, #here-ee", function(){
            displayPage("about");
        });

        $("#main-content").on("click", "#here-robot", function(){
            displayPage("projects"); 
        });
        
        //Page initialization from URL hash
        if (window.location.hash) {
            displayPage(window.location.hash.slice(1));
        } 
        else {
            $("#home-nav").click(); //important
        }
        
        //Popover windows
        $('#example').popover('options');
        $('#element').popover('toggle');
        
        $(function () {
            $('body').popover({
                selector: '[data-toggle="popover"]'
            });

            $('body').tooltip({
                selector: 'a[rel="tooltip"], [data-toggle="tooltip"]'
            });
        });
    });
    
</script>

